# Importing required libraries
import requests
from bs4 import BeautifulSoup
import json

# Defining a function to count words
def count_words(url):
    # Making a request to the URL to get the webpage content
    response = requests.get(url)

    # Parsing the HTML content of the webpage using BeautifulSoup
    soup = BeautifulSoup(response.content, 'html.parser')

    # Extracting all text from the webpage and splitting it into words
    words = soup.get_text().split()

    # Counting the frequency of each word
    counts = {}
    for word in words:
        # Ignoring non-alphabetic characters
        if word.isalpha():
            counts[word] = counts.get(word, 0) + 1

    return counts

# Defining a microservice function
def microservice(url):
    # Calling the count_words function to get the word counts for the input URL
    counts = count_words(url)

    # Formatting the result as a list of dictionaries
    result = []
    for word, count in counts.items():
        result.append({"word": word, "count": count})

    # Converting the result to JSON format
    json_result = json.dumps(result)

    return json_result

# Example usage
if __name__ == '__main__':
    # List of URLs to test the microservice on
    urls = [
        "https://www.nike.com/",
        "https://www.apple.com/",
        "https://www.starbucks.com/",
        "https://www.nationalgeographic.com/",
        "https://www.wikipedia.org/"
    ]

    # Calling the microservice function on each URL and printing the result
    for url in urls:
        print(f"Word counts for {url}:")
        print(microservice(url))
        print()
 
 
